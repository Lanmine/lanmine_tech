# Moltbot environment configuration
# Managed by Ansible - do not edit manually

# LiteLLM proxy
OPENAI_API_KEY={{ moltbot_litellm_api_key }}
OPENAI_API_BASE={{ moltbot_litellm_url }}

# Gateway token
CLAWDBOT_GATEWAY_TOKEN={{ moltbot_secrets.gateway_token }}

# Discord
DISCORD_BOT_TOKEN={{ moltbot_secrets.discord_bot_token }}

# Slack (optional)
SLACK_BOT_TOKEN={{ moltbot_secrets.slack_bot_token | default('') }}
SLACK_APP_TOKEN={{ moltbot_secrets.slack_app_token | default('') }}
